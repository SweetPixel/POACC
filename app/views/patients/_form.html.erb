<%= simple_form_for @patient do |f| %>
<%= f.text_field :Name, :placeholder => "Full Name" %>
<br />
<%= f.text_field :Contact, :placeholder => "Telephone No." %>
<br />
<%= f.text_field :Email, :placeholder => "Email Address" %>
<br />
<%= f.input :Address %>
<br /> 
<div class="form-actions">
  <%= f.button :submit, :class => 'btn btn-primary', :value => "Sign Up" %>
</div>
<% end %>
