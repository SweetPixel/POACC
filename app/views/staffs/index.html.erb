<div class="container main-block">
	<div class="page-header">
		<h1>Our Team</h1>
	</div>
	<div class="row">
		<% @staff.each do |s| %>
		<div class="span3 team-member">
			<h2><%= s.name %></h2>
			<h4><%= s.title %></h4>
			<div class="photo">
				<div class="bordered-img">
					<%= holder_tag "330x210" %>
				</div>
			</div>
			<p><%= s.biography %></p>
		</div>
		<% end %>
	</div>
</div>