<div class="container main-block">
	<div class="page-header">
		<h1>Our Team</h1>
	</div>
	<div class="span12 row">
		<% @staff.each do |s| %>
		<div class="span3 team-member">
			<h2><%= link_to s.name, s %></h2>
			<h4><%= s.title %></h4>
			<div class="photo">
				<div>
					<%= image_tag s.picture.url(:medium), :size => '200x200', :crop => :fill %>
				</div>
			</div>
			
		</div>
		<% end %>
	</div>
</div>